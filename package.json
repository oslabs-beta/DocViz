{
    "name": "docker-stats-app",
    "version": "1.0.0",
    "description": "A tool for monitoring Docker container stats",
    "main": "server/server.js",
    "scripts": {
      "start": "concurrently \"npm run start:backend\" \"npm run start:frontend\"",
      "start:backend": "nodemon server/server.js",
      "start:frontend": "webpack serve --config client/webpack.config.js --mode development",
      "build": "webpack --config client/webpack.config.js",
      "install-client": "cd client && npm install"
    },
    "keywords": [
      "docker",
      "stats",
      "observability",
      "express"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
      "axios": "^1.7.9",
      "body-parser": "^1.20.3",
      "bootstrap": "^5.3.3",
      "chart.js": "^4.4.7",
      "cors": "^2.8.5",
      "dockerode": "^4.0.3",
      "express": "^4.21.2",
      "react": "^19.0.0",
      "react-bootstrap": "^2.10.8",
      "react-bootstrap-icons": "^1.11.5",
      "react-chartjs-2": "^5.3.0",
      "react-dom": "^19.0.0",
      "react-router-dom": "^7.1.3",
      "socket.io": "^4.8.1",
      "websocket": "^1.0.35",
      "ws": "^8.18.0"
    },
    "devDependencies": {
      "@babel/cli": "^7.26.4",
      "@babel/core": "^7.26.0",
      "@babel/preset-env": "^7.26.0",
      "@babel/preset-react": "^7.26.3",
      "babel-loader": "^9.2.1",
      "concurrently": "^9.1.2",
      "css-loader": "^7.1.2",
      "html-webpack-plugin": "^5.6.3",
      "nodemon": "^2.0.22",
      "style-loader": "^4.0.0",
      "webpack": "^5.97.1",
      "webpack-cli": "^6.0.1",
      "webpack-dev-server": "^5.2.0"
    }
  }